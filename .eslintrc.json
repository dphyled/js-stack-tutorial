{
	"env": {
	  "jest": true
	},	
	"extends": [
		"airbnb",
		"plugin:flowtype/recommended"
	],
	"plugins": [
		"flowtype",
		"compat"
	],
	"rules": {
		"semi": [2, "never"],
		"no-unexpected-multiline": 2,
		"linebreak-style": [2, "windows"],
		"compat/compat": 2,
        "indent": [2, "tab"],
        "no-tabs": 0		
	}
}